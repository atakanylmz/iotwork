
@{
    ViewBag.Title = "Ana Sayfa";
}

@using iotWork.Models

@model List<Device>

<div class="row">
    <div class="col-lg-12">
        <section class="panel">
            <header class="panel-heading">
            </header>
            <table class="table table-striped table-advance table-hover">
                <thead>
                    <tr>
                        <th><i class="icon-bullhorn"></i> Cihaz Adı</th>
                        <th class="hidden-phone"><i class="icon-question-sign"></i> IP Adresi</th>
                        <th></th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (Device d in Model)
                    {
                        <tr>
                            <td>@d.deviceName</td>
                            <td class="hidden-phone">@d.deviceIP</td>
                            <td> </td>
                        </tr>

                    }
                </tbody>
            </table>
        </section>
    </div>
</div>
